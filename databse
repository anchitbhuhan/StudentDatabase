





CREATE TABLE `users` (
  `user_id` int(11) NOT NULL,
  `user_first` varchar(256) NOT NULL,
  `user_last` varchar(256) NOT NULL,
  `user_email` varchar(256) NOT NULL,
  `user_uid` varchar(256) NOT NULL,
  `user_pwd` varchar(256) NOT NULL,
  'image' longblob 
)









	create table student(
    usn varchar(11) primary KEY not null,
    name varchar(50) not null,
    mobile varchar(100) not null,
    dob date not null,
    hostelite varchar(3) not null,
    gender varchar(10) not null,
    address varchar(100) not null,                              
    st_image varchar(256) not null
    );


insert into student values(
    '1DT16is001','anchit','8861885327','12-06-2018','yes','m','hazaribagh','abc.png');




create table lib_details(
    id integer(3) PRIMARY KEY not null AUTO_INCREMENT,
    lib_id varchar(10) not null,
    usn varchar(11) not null,
    CONSTRAINT abc FOREIGN key(usn) REFERENCES student(usn) on DELETE CASCADE,
    book_id varchar(10) not null,
    book_name varchar(25) not null,
    date_out date not null,
    due_date date not null
    
    );


insert into lib_details(lib_id,usn,book_id,book_name,date_out,due_date) values(
    '12','1DT16IS001','MAT32','MATHEMATICS',2018-12-12,2018-14-12)



create table course(
    id integer(3) PRIMARY KEY not null AUTO_INCREMENT,
    usn varchar(11) not null,
    CONSTRAINT abcd FOREIGN key(usn) REFERENCES student(usn) on DELETE CASCADE,
    sub_code varchar(10) not null,
    sub_name varchar(20) not null,
    faculty_name varchar(50) not null
    );    



insert into course(usn,sub_code,sub_name,faculty_name) values(
    '1DT16IS001','MAT32','MATHEMATICS','prasad sir')



create table achievements(
    id integer(3) PRIMARY KEY not null AUTO_INCREMENT,	
    usn varchar(11) not null,
    CONSTRAINT abcde FOREIGN KEY(usn) REFERENCES student(usn) on DELETE CASCADE,
    projects varchar(60) not null,
    internships varchar(50) not null,
    certifications varchar(50) not null,
    paper_presntion varchar(100) not null
    
);  

insert INTO achievements(usn,projects,internships,certifications,paper_presntion) VALUES ('1DT16IS001','$project','$internships','$certifications','$paper_present');  


create table acad_details(
     id integer(3) PRIMARY KEY not null AUTO_INCREMENT,
    usn varchar(11) not null,
    CONSTRAINT abcdef FOREIGN key(usn) REFERENCES student(usn) on DELETE CASCADE,
    branch varchar(5) not null,
    sem varchar(5) not null,
    ia1 integer(10) not null,
    ia2 integer(10) not null,
    ia3 integer(10) not null,
    ext_agg float not null
    );


insert into acad_details(usn,branch,sem,ia1,ia2,ia3,ext_agg) values ('1DT16IS001','ise','5',15,14,15,12.08);









